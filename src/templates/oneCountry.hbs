{{#each this}}
<div class="country_card">
    <article class="card">
        <h2 class="country_name">{{name}}</h2>
        <p>Capital {{capital}}</p>
        <p>Population{{population}}</p>
        <p>Languages</p>

        <img src="{{flag}}" alt="Flag of {{name}}" class="flag_image" />
    </article>
</div>
{{/each}}